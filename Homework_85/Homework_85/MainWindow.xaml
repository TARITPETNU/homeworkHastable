<Window x:Class="Homework_85.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Homework_85"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="23" Margin="129,100,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="499"/>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="23" Margin="129,128,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="499"/>
        <Button x:Name="removeBtn" Content="Remove" HorizontalAlignment="Left" Margin="323,156,0,0" VerticalAlignment="Top" Width="75" Click="removeBtn_Click"/>
        <Button x:Name="addBtn" Content="Add" HorizontalAlignment="Left" Margin="243,156,0,0" VerticalAlignment="Top" Width="75" Click="addBtn_Click"/>
        <Button x:Name="showBtn" Content="show all" HorizontalAlignment="Left" Margin="403,156,0,0" VerticalAlignment="Top" Width="75" Click="showBtn_Click"/>

    </Grid>
</Window>
